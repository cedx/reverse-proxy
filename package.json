{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/reverse-proxy.js/issues",
  "description": "Simple reverse proxy server supporting WebSockets.",
  "homepage": "https://github.com/cedx/reverse-proxy.js",
  "license": "MIT",
  "main": "./index",
  "name": "reverse-proxy-js",
  "preferGlobal": true,
  "version": "0.4.0",
  "bin": {
    "reverse-proxy": "./bin/cli.js"
  },
  "dependencies": {
    "commander": "^2.6.0",
    "forever": "^0.14.1",
    "http-proxy": "^1.8.1"
  },
  "devDependencies": {
    "del": "^1.1.1",
    "docgen-js": "^0.0.1",
    "gulp": "^3.8.11",
    "gulp-david": "^0.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-load-plugins": "^0.8.0",
    "gulp-zip": "^2.0.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": "nofunc",
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "http",
    "reverse",
    "routing",
    "proxy",
    "server",
    "websocket"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cedx/reverse-proxy.js.git"
  },
  "scripts": {
    "start": "forever start -c node -e var/stderr.log -o var/stdout.log -l var/forever.log --append --minUptime 30000 --spinSleepTime 30000 --uid reverse-proxy bin/cli.js",
    "stop": "forever stop reverse-proxy"
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/reverse-proxy.js/favicon.ico",
    "name": "Reverse-Proxy.js",
    "url": "https://www.npmjs.com/package/reverse-proxy-js",
    "options": {
      "linkNatives": true,
      "outdir": "doc/api",
      "paths": [ "lib" ],
      "tabtospace": 2,
      "theme": "default"
    }
  }
}
