{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/reverse-proxy.js/issues",
  "description": "Simple reverse proxy server supporting WebSockets.",
  "homepage": "https://github.com/cedx/reverse-proxy.js",
  "license": "MIT",
  "main": "./index",
  "name": "reverse-proxy-js",
  "preferGlobal": true,
  "version": "0.1.0",
  "bin": {
    "reverse-proxy": "./bin/cli.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "commander": "^2.3.0",
    "http-proxy": "^1.1.6"
  },
  "devDependencies": {
    "docgen-js": "^0.1.0",
    "jshint": "^2.5.2",
    "nodemon": "^1.2.1"
  },
  "engines": {
    "node": ">=0.10.30"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": true,
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "http",
    "reverse",
    "routing",
    "proxy",
    "server",
    "websocket"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cedx/reverse-proxy.js.git"
  },
  "scripts": {
    "make": "node bin/make.js",
    "start": "node bin/cli.js"
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/reverse-proxy.js/favicon.ico",
    "name": "Reverse-Proxy.js",
    "url": "https://github.com/cedx/reverse-proxy.js",
    "options": {
      "linkNatives": true,
      "outdir": "doc/api",
      "paths": [ "bin", "lib" ],
      "tabtospace": 2
    }
