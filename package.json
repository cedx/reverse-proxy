{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/reverse-proxy.js/issues",
  "description": "Simple reverse proxy server supporting WebSockets and hostname-based routing tables.",
  "homepage": "https://github.com/cedx/reverse-proxy.js",
  "license": "MIT",
  "main": "./index",
  "name": "reverse-proxy-js",
  "preferGlobal": true,
  "private": false,
  "version": "0.1.0",
  "bin": {
    "reverse-proxy": "./bin/cli.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "commander": "^2.3.0",
    "forever": "^0.11.1",
    "http-proxy": "^1.1.6"
  },
  "devDependencies": {
    "jshint": "^2.5.2",
    "yuidocjs": "^0.3.50"
  },
  "engines": {
    "node": ">=0.10.30"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": true,
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "http",
    "reverse",
    "routing",
    "proxy",
    "server",
    "websocket"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cedx/reverse-proxy.js.git"
  },
  "scripts": {
    "doc": "yuidoc --lint && yuidoc --no-code",
    "lint": "jshint --verbose bin lib",
    "start": "forever start -c node -e var/stderr.log -o var/stdout.log -l reverse-proxy.log --append --minUptime 15000 --spinSleepTime 15000 --uid reverse-proxy bin/cli.js --config etc",
    "stop": "forever stop reverse-proxy"
  }
}
