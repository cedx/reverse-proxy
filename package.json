{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/reverse-proxy.js/issues",
  "description": "Simple reverse proxy server supporting WebSockets.",
  "homepage": "https://github.com/cedx/reverse-proxy.js",
  "license": "MIT",
  "main": "./index",
  "name": "reverse-proxy-js",
  "preferGlobal": true,
  "version": "0.2.1",
  "bin": {
    "reverse-proxy": "./bin/cli.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "commander": "^2.3.0",
    "david": "^5.0.0",
    "http-proxy": "^1.5.3",
    "shelljs": "^0.3.0"
  },
  "devDependencies": {
    "archiver": "^0.11.0",
    "docgen-js": "^0.1.0",
    "jshint": "^2.5.6",
    "nodemon": "^1.2.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
